{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
   "types": ["node", "jest", "@testing-library/jest-dom"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
  },
  "include": ["src"]
}